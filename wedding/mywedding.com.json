{
  "name":"crawl mywedding.com deals",
  "model": "com.fp.wedding.model.Provider",
  "urls":[
    "http://www.mywedding.com/la?page=1 !! category=wedding"
  ],
  "list_xpath":"//div[contains(@id, 'container-listings')]//div[contains(@class, 'search-listing')]",
  "fields":{
    "siteName": "##www.mywedding.com",
    "title": "//div[contains(@class, 'business-info')]//div[contains(@class, 'business-name')]",
    "category": "$$category",
    "imageUrl": "//div[contains(@class, 'span3')]//a//img/@src",
    "desc": "//div[contains(@class, 'span9')]//div[contains(@class, 'row-fluid')]//p",
    "phoneNum": "//div[contains(@class, 'business-info')]//ul[contains(@class, 'contact-links')]//li[2]//span/@data-phone",
    "detailUrl": "//div[contains(@class, 'business-info')]//div[contains(@class, 'business-name')]//a/@href",
    "numOfLike":"//div[contains(@class, 'text-right')]//span[contains(@class, 'love-count')]",
	"website": "//div[contains(@class, 'span3')]//p[contains(@class,'text-center')]//a[contains(@class,'vendor-link')]/@href"
  }
}
