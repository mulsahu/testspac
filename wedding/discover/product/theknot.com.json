{
  "name": "product:theknot.com",
  "model": "com.fp.crawler.model.Product",
  "fields": {
    "attachUrls" : "ul[class=carousel-hero-list list-unstyled] li img/@data-src",
    "phoneNum" : "div/@data-analytics-vendor-formatted-phone-number !! ([0-9 A-Z a-z]+)",
    "website" : "a[id=website-link]/@href",
    "address" : "div[class=address], div[class=service_area]",
    "city" : "div/@data-analytics-vendor-city !! ([0-9 A-Z a-z]+)",
    "state" : "div/@data-analytics-vendor-state !! ([0-9 A-Z a-z]+)",
    "zipcode" : "div/@data-analytics-vendor-zip !! ([0-9 A-Z a-z]+)",
    "rating" : "div[id=vendor-info] meta/@content"
  }
}
